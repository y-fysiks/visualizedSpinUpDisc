<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbox" alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visualizedspinupdisc.visualizedspinupdisc.MainController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <HBox alignment="CENTER" spacing="40.0">
    <TextField fx:id="velocity" alignment="TOP_LEFT" focusTraversable="false" prefWidth="90.0" promptText="Velocity (m/s)" />
    <TextField fx:id="angle" alignment="TOP_LEFT" focusTraversable="false" layoutX="185.0" layoutY="22.0" prefWidth="90.0" promptText="Angle (deg)" />
    <TextField fx:id="height" alignment="TOP_LEFT" focusTraversable="false" layoutX="650.0" layoutY="10.0" prefWidth="90.0" promptText="Height (in)" />
  </HBox>
  <Button fx:id="calculateButton" mnemonicParsing="false" onAction="#calculatePressed" text="Calculate" />
</VBox>
